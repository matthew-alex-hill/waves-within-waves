play mainWave
sineWave
squareWave

mainWave.shape = EMPTY
mainWave.base = SUM sineWave squareWave

sineWave.shape = SINE
sineWave.frequency = in.frequency
sineWave.amplitude = in.velocity
sineWave.attack = 0.3

squareWave.shape = SQUARE
squareWave.frequency = in.frequency
squareWave.amplitude = in.velocity
squareWave.attack = 0.3
